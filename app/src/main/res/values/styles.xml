 <resources>

    <!-- Base application theme. -->
    <style name="AppTheme.ObjectDetection" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/tfe_color_primary</item>
        <item name="colorPrimaryDark">@color/tfe_color_primary_dark</item>
        <item name="colorAccent">@color/tfe_color_accent</item>
    </style>

     <style name="toolbartex" parent="TextAppearance.MaterialComponents.Button">

         <item name="android:textSize">25dp</item>
         <item name="android:textAllCaps">false</item>
         <item name="android:textStyle">bold</item>
     </style>

     <style name="Foxandroid.Toolbar" parent="Widget.AppCompat.Toolbar">
         <item name="android:background">@color/tfe_color_primary_dark</item>
         <item name="theme">@style/AppTheme.ObjectDetection</item>
         <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
         <item name="titleTextAppearance">@style/toolbartex</item>

     </style>

</resources>
